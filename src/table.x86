	d1 fa                   # sarl   %edx
	8b 2d [F]               # movl   FLAGS,%ebp
	83 e5 0c                # andl   $0x0c,%ebp
	83 cd 30                # orl    $0x30,%ebp
	83 0d [F] 04            # orl    $0x04,FLAGS
	09 ea                   # orl    %ebp,%edx
